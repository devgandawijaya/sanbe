<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">

        <record id="prinsipal_channel_form" model="ir.ui.view">
            <field name="name">prinsipal.channel.form</field>
            <field name="model">prinsipal.channel</field>
            <field name="arch" type="xml">
                <form string="Pricipal Channel">
                    <sheet>
                        <group>
                            <field name="kode_principal_chanel"/>
                            <field name="kode_principal"/>
                            <field name="nama_chanel"/>
                            <field name="time_stamp"/>
                            <field name="nama_principal_id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        
        <record id="prinsipal_channel_tree" model="ir.ui.view">
            <field name="name">prinsipal.channel.tree</field>
            <field name="model">prinsipal.channel</field>
            <field name="arch" type="xml">
                <tree string="Pricipal Channel">
                    <field name="kode_principal_chanel"/>
                    <field name="kode_principal"/>
                    <field name="nama_chanel"/>
                    <field name="time_stamp"/>
                    <field name="nama_principal_id"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>
