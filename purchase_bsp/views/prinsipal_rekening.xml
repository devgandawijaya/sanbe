<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">

        <record id="prinsipal_rekening_form" model="ir.ui.view">
            <field name="name">prinsipal.rekening.form</field>
            <field name="model">prinsipal.rekening</field>
            <field name="arch" type="xml">
                <form string="Prinsipal Rekening">
                    <sheet>
                        <group>
                            <field name="kode_rekening_principal"/>
                            <field name="kode_bank"/>
                            <field name="kode_principal"/>
                            <field name="an_rekening_principal"/>
                            <field name="nama_bank"/>
                            <field name="karyawan_id"/>
                            <field name="time_stamp"/>
                            <field name="nomor_rekening"/>
                            <field name="nama_principal_id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        
        <record id="prinsipal_rekening_tree" model="ir.ui.view">
            <field name="name">prinsipal.rekening.tree</field>
            <field name="model">prinsipal.rekening</field>
            <field name="arch" type="xml">
                <tree string="Prinsipal Rekening">
                    <field name="kode_rekening_principal"/>
                    <field name="kode_bank"/>
                    <field name="kode_principal"/>
                    <field name="an_rekening_principal"/>
                    <field name="nama_bank"/>
                    <field name="karyawan_id"/>
                    <field name="time_stamp"/>
                    <field name="nomor_rekening"/>
                    <field name="nama_principal_id"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>
