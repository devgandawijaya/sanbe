<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">

        <record id="prinsipal_pajak_form" model="ir.ui.view">
            <field name="name">prinsipal.pajak.form</field>
            <field name="model">prinsipal.pajak</field>
            <field name="arch" type="xml">
                <form string="Prinsipal Pajak">
                    <sheet>
                        <group>
                            <field name="kode_pajak_prinsipal"/>
                            <field name="kode_principal"/>
                            <field name="tanggal_pengajuan"/>
                            <field name="status_pajak"/>
                            <field name="karyawan_id"/>
                            <field name="time_stamp"/>
                            <field name="time_create"/>
                            <field name="nama_principal_id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        
        <record id="prinsipal_pajak_tree" model="ir.ui.view">
            <field name="name">prinsipal.pajak.tree</field>
            <field name="model">prinsipal.pajak</field>
            <field name="arch" type="xml">
                <tree string="Prinsipal Pajak">
                    <field name="kode_pajak_prinsipal"/>
                    <field name="kode_principal"/>
                    <field name="tanggal_pengajuan"/>
                    <field name="status_pajak"/>
                    <field name="karyawan_id"/>
                    <field name="time_stamp"/>
                    <field name="time_create"/>
                    <field name="nama_principal_id"/>
                </tree>
            </field>
        </record>


    </data>
</odoo>
