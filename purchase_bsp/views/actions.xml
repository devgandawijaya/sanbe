<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="action_prinsipal_channel" model="ir.actions.act_window">
            <field name="name">prinsipal channel</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">prinsipal.channel</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No prinsipal channel
                </p>
                <p>
                    Create new prinsipal channel.
                </p>
            </field>
        </record>

        <record id="action_redirect_to_import_prinsipal_channel" model="ir.actions.server">
            <field name="name">Redirect to Import Product Channel</field>
            <field name="model_id" ref="model_prinsipal_channel"/>
            <field name="state">code</field>
            <field name="code">
                action = {
                    'type': 'ir.actions.act_url',
                    'url': '/import/prinsipal/channel',
                    'target': 'self',
                }
            </field>
        </record>

