<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">

        <record id="cabang_mapping_form" model="ir.ui.view">
            <field name="name">cabang.mapping.form</field>
            <field name="model">cabang.mapping</field>
            <field name="arch" type="xml">
                <form string="Cabang Mapping">
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        
        <record id="cabang_mapping_tree" model="ir.ui.view">
            <field name="name">cabang.mapping.tree</field>
            <field name="model">cabang.mapping</field>
            <field name="arch" type="xml">
                <tree string="Cabang Mapping">
                    <field name="name"/>
                </tree>
            </field>
        </record>


    </data>
</odoo>
