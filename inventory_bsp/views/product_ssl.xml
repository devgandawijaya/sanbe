<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="product_ssl_form" model="ir.ui.view">
            <field name="name">product.ssl.form</field>
            <field name="model">product.ssl</field>
            <field name="arch" type="xml">
                <form string="Product ssl">
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        
        <record id="product_ssl_tree" model="ir.ui.view">
            <field name="name">product.ssl.tree</field>
            <field name="model">product.ssl</field>
            <field name="arch" type="xml">
                <tree string="Product ssl">
                    <field name="name"/>
                </tree>
            </field>
        </record>

    </data>
</odoo>
