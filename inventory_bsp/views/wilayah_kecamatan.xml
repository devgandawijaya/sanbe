<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">

        <record id="wilayah_kecamatan_form" model="ir.ui.view">
            <field name="name">wilayah.kecamatan.form</field>
            <field name="model">wilayah.kecamatan</field>
            <field name="arch" type="xml">
                <form string="Wilayah kecamatan">
                    <sheet>
                        <group>
                            <field name="id_kecamatan"/>
                            <field name="kode_kabkot_id"/>
                            <field name="nama_kecamatan"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        
        <record id="wilayah_kecamatan_tree" model="ir.ui.view">
            <field name="name">wilayah.kecamatan.tree</field>
            <field name="model">wilayah.kecamatan</field>
            <field name="arch" type="xml">
                <tree string="Wilayah kecamatan">
                    <field name="id_kecamatan"/>
                    <field name="kode_kabkot_id"/>
                    <field name="nama_kecamatan"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>
